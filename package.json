{
  "name": "~snowpack-react-starter",
  "version": "0.0.0-dev",
  "private": true,
  "scripts": {
    "dev": "snowpack dev",
    "start": "snowpack dev",
    "build": "snowpack build",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "npm run eslint && npm run test:r",
    "test:r": "wtr src/**/*.test.tsx --node-resolve",
    "test:w": "npm run test:r -- --watch",
    "format": "prettier --write ."
  },
  "dependencies": {
    "core-js": "^3.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@snowpack/plugin-postcss": "^1.0.7",
    "@snowpack/plugin-react-refresh": "^2.3.5",
    "@snowpack/plugin-webpack": "^2.1.0",
    "@snowpack/web-test-runner-plugin": "^0.1.3",
    "@testing-library/react": "^11.1.1",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/snowpack-env": "^2.3.1",
    "@typescript-eslint/parser": "^4.6.1",
    "@web/test-runner": "^0.9.7",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "cssnano-preset-default": "^4.0.7",
    "eslint": "^7.12.1",
    "eslint-plugin-only-warn": "^1.0.2",
    "kleur": "^4.1.3",
    "postcss": "^8.1.6",
    "postcss-center": "^1.1.0",
    "postcss-cli": "^8.2.0",
    "prettier": "^2.1.2",
    "snowpack": "^2.16.1",
    "tailwindcss": "^1.9.6",
    "typescript": "^4.0.5"
  }
}
