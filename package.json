{
  "name": "snowpack-react-starter",
  "version": "0.0.0-dev",
  "private": true,
  "scripts": {
    "dev": "snowpack dev",
    "start": "snowpack dev",
    "build": "sh Taskfile build",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "npm run eslint && npm run test:cov",
    "test:r": "web-test-runner src/**/*.test.tsx --node-resolve",
    "test:w": "npm run test:r -- --watch",
    "test:cov": "npm run test:r -- --coverage",
    "tscheck:w": "tsc --watch --noEmit",
    "format": "prettier --write ."
  },
  "dependencies": {
    "core-js": "^3.8.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@snowpack/plugin-postcss": "^1.0.11",
    "@snowpack/plugin-react-refresh": "^2.3.9",
    "@snowpack/plugin-webpack": "^2.2.0",
    "@snowpack/web-test-runner-plugin": "^0.1.5",
    "@testing-library/react": "^11.2.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/snowpack-env": "^2.3.3",
    "@typescript-eslint/parser": "^4.10.0",
    "@web/test-runner": "^0.11.0",
    "cssnano": "^4.1.10",
    "cssnano-preset-default": "^4.0.7",
    "eslint": "^7.16.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "expect": "^26.6.2",
    "postcss": "^8.2.1",
    "postcss-center": "^1.1.0",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.2.1",
    "snowpack": "^2.18.5",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3"
  }
}
